<div class="container">
  <div class="row">
    <div class="col-sm-8 col-sm-offset-2" class="justify-content-center">
      <% @articles.each do |article| %>
      <h3><%= link_to article.title, article_path(article) %></h3>
      <p><%= article.content %> </p>
      <% end %>
    </div>
  </div>
</div>
